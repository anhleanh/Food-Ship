<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ststudio.foodship">

    <application
        android:name=".AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mainmenu.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.ui.SignupActivity" />
        <activity android:name=".login.ui.LoginActivity" />
        <activity android:name=".mainmenu.ui.StoreActivity" />
        <activity android:name=".mainmenu.ui.FoodListByStoreActivity"></activity>
    </application>

</manifest>